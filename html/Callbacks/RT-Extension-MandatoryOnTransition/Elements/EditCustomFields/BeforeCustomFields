<%args>
$CustomFields
$ARGSRef
</%args>
<%init>
return unless $ARGSRef->{'Named'} and @{$ARGSRef->{'Named'}};

$CustomFields->Limit( FIELD => 'Name',
                      VALUE => $_,
                      SUBCLAUSE => 'names',
                      ENTRYAGGREGRATOR => 'OR',
                      CASESENSITIVE => 1 )
    for @{$ARGSRef->{'Named'}};
</%init>
